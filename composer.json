{
    "name": "novosga/novosga",
    "description": "Support queue management system",
    "type": "project",
    "homepage": "http://novosga.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Rog√©rio Lino",
            "email": "rogeriolino@gmail.com"
        },
        {
            "name": "NovoSGA Community",
            "homepage": "https://github.com/novosga/novosga/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "friendsofsymfony/oauth-server-bundle": "^1.6",
        "jms/serializer": "^1.9",
        "novosga/attendance-bundle": "^2.0",
        "novosga/composer-installer-plugin": "v1.0",
        "novosga/core": "^2.0",
        "novosga/monitor-bundle": "^2.0",
        "novosga/reports-bundle": "^2.0",
        "novosga/settings-bundle": "^2.0",
        "novosga/triage-bundle": "^2.0",
        "novosga/users-bundle": "^2.0",
        "novosga/websocket-server": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/expression-language": "^4.1",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^4.1",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/web-link": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1",
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
