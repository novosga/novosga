<!DOCTYPE html>
<html>
<head>
    <title><?php echo $atendimento->getSenha() ?></title>
    <link type="text/css" rel="stylesheet" href="{{ baseUrl() }}/css/style.css?v={{ version }}" />
    <link type="text/css" rel="stylesheet" href="{{ baseUrl() }}/{{ modulo.getPath }}/css/print.css?v={{ version }}" />
    <link rel="shortcut icon" href="images/favicon.png" />
</head>
<body onload="window.print(); window.close();">
    <div id="senha">
        <div id="senha-header">
            {{ unidade.nome }}<br />
            <span class="data">{{ data }}</span>
        </div>
        <div id="senha-body">
            {{ atendimento.senha }}
            <span class="descricao">
                {{ atendimento.senha.prioridade.nome }}
            </span>
        </div>
        <div id="senha-footer">
            {{ unidade.mensagemImpressao }}
        </div>
    </div>
</body>
</html>