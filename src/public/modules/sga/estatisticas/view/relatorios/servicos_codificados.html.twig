{% for dado in relatorio.dados %}
<div class="header">
    <h2>{{ dado.unidade }}</h2>
    <p>{% trans %}Período de %s a %s{% endtrans %}</p>
</div>
<table class="ui-data-table">
    <thead>
        <tr>
            <th>{% trans %}Serviço{% endtrans %}</th>
            <th>{% trans %}Total{% endtrans %}</th>
        </tr>
    </thead>
    <tbody>
        {% for s in dado.servicos %}
        <tr>
            <td>{{ s.nome }}</td>
            <td>{{ s.total }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% endfor %}